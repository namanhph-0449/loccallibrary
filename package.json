{
  "name": "express-locallibrary",
  "version": "0.0.0",
  "main": "src/app.ts",
  "private": true,
  "scripts": {
    "start": "ts-node /dist/app.ts",
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate -- -d src/config/data-source.ts src/migration/InitialMigration",
    "migration:run": "npm run typeorm migration:run -- -d src/config/data-source.ts"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/debug": "^4.1.12",
    "@types/morgan": "^1.9.9",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^3.10.3",
    "pug": "2.0.0-beta11",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.11",
    "nodemon": "^3.1.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3"
  }
}
